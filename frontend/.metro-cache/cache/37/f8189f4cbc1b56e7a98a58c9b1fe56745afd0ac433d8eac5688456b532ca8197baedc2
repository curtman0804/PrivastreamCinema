Interval(pollIntervalRef.current);\n              }\n              return;\n            }\n            pollIntervalRef.current = setTimeout(pollStatus, pollInterval);\n          } catch (err) {\n            console.error('Status poll error:', err);\n            pollIntervalRef.current = setTimeout(pollStatus, 2000);\n          }\n        };\n        pollStatus();\n      } catch (err) {\n        console.error('Stream start error:', err);\n        setError(err.message || 'Failed to start stream');\n        setIsLoading(false);\n      }\n    };\n    const formatSpeed = bytesPerSec => {\n      if (bytesPerSec > 1024 * 1024) {\n        return `${(bytesPerSec / 1024 / 1024).toFixed(1)} MB/s`;\n      }\n      return `${(bytesPerSec / 1024).toFixed(0)} KB/s`;\n    };\n\n    // Handle back button\n    const handleBack = () => {\n      router.back();\n    };\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n      style: styles.container,\n      children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(StatusBar.default, {\n        hidden: true\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 893,\n        columnNumber: 7\n      }, this), isLoading && !error && /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.stremioLoadingContainer,\n        children: [(backdrop || poster) && /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Image.default, {\n          source: {\n            uri: backdrop || poster\n          },\n          style: styles.loadingBackdrop,\n          blurRadius: Platform.default.OS === 'web' ? 0 : 3\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 900,\n          columnNumber: 13\n        }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.loadingDarkOverlay\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 908,\n          columnNumber: 11\n        }, this), /*#_